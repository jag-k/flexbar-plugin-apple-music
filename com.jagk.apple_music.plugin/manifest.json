{
  "name": "Apple Music",
  "uuid": "com.jagk.apple_music",
  "version": "1.0.0",
  "author": "Jag_k",
  "entry": "backend/plugin.cjs",
  "description": "Control Apple Music via FlexBar!",
  "repo": "https://github.com/jag-k/flexbar-plugin-apple-music",
  "configPage": "config_page",
  "sdk": {
    "version": "1.0.0"
  },
  "software": {
    "minimumVersion": "1.0.0"
  },
  "os": [
    {
      "platform": "mac",
      "minimumVersion": "10.15"
    }
  ],
  "device": [
    {
      "name": "flexbar",
      "version": "1.0.0"
    }
  ],
  "keyLibrary": {
    "title": "$PluginName",
    "style": {
      "icon": "mdi mdi-apple"
    },
    "children": [
      {
        "title": "$TrackInfo.Title",
        "tip": "$TrackInfo.Tip",
        "cid": "com.jagk.apple_music.trackInfo",
        "config": {
          "keyType": "default",
          "clickable": true,
          "platform": [
            "mac"
          ]
        },
        "style": {
          "icon": "mdi mdi-music",
          "borderStyle": "solid",
          "borderWidth": 3,
          "borderColor": "#fa243c",
          "bgColor": "#000000",
          "width": 420,
          "iconPos": {
            "X": 8,
            "Y": 49
          },
          "showTitle": true,
          "titlePos": {
            "X": 55,
            "Y": 50
          }
        },
        "data": {
          "showArtwork": true
        }
      },
      {
        "title": "$PlayPause.Title",
        "tip": "$PlayPause.Tip",
        "cid": "com.jagk.apple_music.playPause",
        "config": {
          "keyType": "multiState",
          "clickable": true,
          "platform": [
            "mac"
          ]
        },
        "style": {
          "icon": "mdi mdi-play-pause",
          "width": 60,
          "showTitle": false,
          "emoji": "‚èØ\uFE0F",
          "borderStyle": "solid",
          "borderWidth": 3,
          "borderColor": "#fa243c",
          "bgColor": "#000000",
          "multiStyle": [
            {
              "icon": "mdi mdi-play-pause",
              "fgColor": "#ffffff80"
            },{
              "icon": "mdi mdi-play",
              "fgColor": "#ffffffff"
            },
            {
              "icon": "mdi mdi-pause",
              "fgColor":  "#ffffffff"
            }
          ]
        },
        "data": {}
      },
      {
        "title": "$Next.Title",
        "tip": "$Next.Tip",
        "cid": "com.jagk.apple_music.next",
        "config": {
          "keyType": "default",
          "clickable": true,
          "platform": [
            "mac"
          ]
        },
        "style": {
          "icon": "mdi mdi-skip-next",
          "width": 60,
          "showTitle": false,
          "borderStyle": "solid",
          "borderWidth": 3,
          "borderColor": "#fa243c",
          "bgColor": "#000000"
        },
        "data": {}
      },
      {
        "title": "$Previous.Title",
        "tip": "$Previous.Tip",
        "cid": "com.jagk.apple_music.previous",
        "config": {
          "keyType": "default",
          "clickable": true,
          "platform": [
            "mac"
          ]
        },
        "style": {
          "icon": "mdi mdi-skip-previous",
          "width": 60,
          "showTitle": false,
          "borderStyle": "solid",
          "borderWidth": 3,
          "borderColor": "#fa243c",
          "bgColor": "#000000"
        },
        "data": {}
      }
    ]
  },
  "local": {
    "en": {
      "PluginName": "Apple Music",
      "config": {
        "updateRate": {
          "name": "Update Rate",
          "description": "How often the plugin should check for updates (in milliseconds)"
        }
      },
      "TrackInfo": {
        "Title": "Track Info",
        "Tip": "Displays current track from Apple Music",
        "UI": {
          "ShowArtwork": {
            "Label": "Show Artwork",
            "Tip": "If set, then will replacing icon with artwork image"
          }
        }
      },
      "PlayPause": {
        "Title": "Play/Pause",
        "Tip": "Toggle play/pause for Apple Music"
      },
      "Next": {
        "Title": "Next Track",
        "Tip": "Switch to next track in Apple Music"
      },
      "Previous": {
        "Title": "Previous Track",
        "Tip": "Switch to previous track in Apple Music"
      }
    }
  }
}
